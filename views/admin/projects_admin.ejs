<div class="admin-title">
  <a href="/dashboard">&larr; Back</a>
  <h2>Projects</h2>
  <a href="/add-project" class='button'>+ Add New</a>
</div>

<ul class="admin-posts">
<%data.forEach(Project=>{%>
  <li>
    <a href="/projects/<%=Project._id%>">
      <%=Project.title%> &nearr;
      </a>
      <div class="admin-post-controls">
        <span><%=Project.createdAt.toDateString()%></span>
<a href="/edit-project/<%=Project._id%>" class='btn'>Edit</a>
<form class="" action="/delete-project/<%=Project._id%>?_method=DELETE" method="POST">
<input type="submit" class="btn" value="Delete">
</form>
      </div>
  </li>
<%})%>



</ul>
